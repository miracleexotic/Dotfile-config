{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "display_host": false,
            "display_user": true
          },
          "style": "plain",
          "template": "üöÄ {{ if .SSHSession }}\uf817 {{ end }}<#B6A5FF>{{ .UserName }}</> ",
          "type": "session"
        },
        {
          "foreground": "cyan",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 2
          },
          "style": "plain",
          "template": "<#ffffff>at</> <#e5e5e5,transparent>\ue0b6</><transparent,#e5e5e5><b>Ôùï {{ .Path }}</b></><#e5e5e5,transparent>\ue0b4</> ",
          "type": "path"
        },
        {
          "type": "poshgit",
          "style": "plain",
          "foreground": "#b98eff",
          "template": "<#ffffff>on</> <b>üå± </b><#b98eff>{{ .Status }}</>"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "plain",
          "template": "<#6CA35E,transparent> \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}</>",
          "type": "node"
        },
        {
          "background": "#f78fb3",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files"
          },
          "style": "plain",
          "template": "<#ffffff,transparent> \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}</>",
          "type": "python"
        },
        {
          "background": "#7FD5EA",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "plain",
          "template": "<#7FD5EA,transparent> \ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</>",
          "type": "go"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<lightGreen>\u276f </>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
